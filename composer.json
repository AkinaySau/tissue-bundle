{
    "name": "sau/tissue-bundle",
    "type": "symfony-bundle",
    "description": "Slight variation of the original cleentfaar/CLTissueBundle and evozon-php/tissue-bundle",
    "keywords": [
        "anti-virus",
        "security",
        "tissue",
        "virus",
        "clamav"
    ],
    "homepage": "https://github.com/AkinaySau",
    "license": "proprietary",
    "authors": [
        {
            "name": "Cas Leentfaar",
            "homepage": "https://github.com/cleentfaar",
            "email": "info@casleentfaar.com"
        },
        {
            "name": "Balazs Csaba",
            "homepage": "http://evozon.com",
            "email": "csaba.balazs@evozon.com"
        },
        {
            "name": "Vladimir Mogilev",
            "homepage": "https://github.com/AkinaySau",
            "email": "akinaysau@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "~4.0",
        "symfony/options-resolver": "~4.0",
        "symfony/validator": "~4.0",
        "symfony/event-dispatcher": "~4.0",
        "sau/tissue": ">=1.0.0",
        "sau/tissue-clamav-adapter": ">=1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "matthiasnoback/symfony-config-test": "^3.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CL\\Bundle\\TissueBundle\\": ""
        }
    }
}
